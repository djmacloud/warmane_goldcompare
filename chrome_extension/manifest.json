{
  "name": "Warmane Gold Compare Extension",
  "description": "This extension compares gold sells on warmane.com website and tells you wich one worth more so you get maximum gold with your coins.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/wow16.png",
      "32": "/images/wow32.png",
      "48": "/images/wow48.png",
      "128": "/images/wow128.png"
    }
  },
  "icons": {
    "16": "/images/wow16.png",
    "32": "/images/wow32.png",
    "48": "/images/wow48.png",
    "128": "/images/wow128.png"
  }
  // "options_page": "options.html"
}
